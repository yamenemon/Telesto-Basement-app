CONFIGURATION_BUILD_DIR = $PODS_CONFIGURATION_BUILD_DIR/DRColorPicker
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/DRColorPicker" "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CNPPopupController" "${PODS_ROOT}/Headers/Public/DRColorPicker"
OTHER_LDFLAGS = -framework "ImageIO" -framework "QuartzCore" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}
PODS_TARGET_SRCROOT = ${PODS_ROOT}/DRColorPicker
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
