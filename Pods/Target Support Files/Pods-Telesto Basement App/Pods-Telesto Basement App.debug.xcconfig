GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CNPPopupController" "${PODS_ROOT}/Headers/Public/DRColorPicker"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/CNPPopupController" "$PODS_CONFIGURATION_BUILD_DIR/DRColorPicker"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CNPPopupController" -isystem "${PODS_ROOT}/Headers/Public/DRColorPicker"
OTHER_LDFLAGS = $(inherited) -ObjC -l"CNPPopupController" -l"DRColorPicker" -framework "ImageIO" -framework "QuartzCore" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
